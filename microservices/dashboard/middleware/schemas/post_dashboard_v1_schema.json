{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Session Schema",
  "type": "object",
  "required": [
    "title"
  ],
  "properties": {
    "id": {
      "type": "string",
      "minLength": 0,
      "maxLength": 125
    },
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 250
    },
    "completed": {
      "type": "boolean",
      "enum": [
        true,
        false
      ]
    }
  }
}
