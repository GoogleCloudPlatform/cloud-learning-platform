{ 
    "$schema":"http://json-schema.org/draft-04/schema#",
    "title":"Update Sub Competency Schema",
    "type":"object",
    "required":[
        "title",
        "all_learning_resource",
        "label"
    ],
    "properties":{ 
        "title":{ 
            "type":"string",
            "minLength":1,
            "maxLength":100
        },
        "description":{ 
            "type":"string",
            "minLength":0,
            "maxLength":10000
        },
        "all_learning_resource":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "parent_node":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "label":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "total_lus":{
            "type":"number"
        },
        "is_valid":{
            "type":"boolean"
        },
        "created_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "last_modified_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        }
        
    }
}
