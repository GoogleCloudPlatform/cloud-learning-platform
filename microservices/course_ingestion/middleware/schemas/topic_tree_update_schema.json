{ 
    "$schema":"http://json-schema.org/draft-04/schema#",
    "title":"Schema for Updating topic tree",
    "type":"object",
    "required":[ 
        "file_path"
    ],
    "properties":{ 
        "file_path":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "created_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "last_modified_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        }
    }
}
