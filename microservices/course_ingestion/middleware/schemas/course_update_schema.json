{ 
    "$schema":"http://json-schema.org/draft-04/schema#",
    "title":"Update Course",
    "type":"object",
    "properties":{
        "title":{ 
            "type":"string",
            "minLength":1,
            "maxLength":1000
        },
        "label":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "is_valid":{
            "type":"boolean"
        },
        "competency_ids":{
            "type":"array",
            "items": {
                "type":"string"
            }
        },
        "override_existing_competencies":{
            "type":"boolean"
        },
        "created_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "last_modified_by":{ 
            "type":"string",
            "minLength":0,
            "maxLength":1000
        },
        "course_category":{
            "enum": [
                "default",
                "bio"
            ]
        }
    }
}
